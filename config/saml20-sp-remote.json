[
    {
        "AssertionConsumerService": {
            "Binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST", 
            "Location": "{BASE_URL}/sspsp/module.php/saml/sp/saml2-acs.php/default-sp"
        }, 
        "IDPList": [
            "{BASE_URL}/sspidp/saml2/idp/metadata.php"
        ], 
        "NameIDFormat": "urn:oasis:names:tc:SAML:2.0:nameid-format:persistent", 
        "SingleLogoutService": "{BASE_URL}/sspsp/module.php/saml/sp/saml2-logout.php/default-sp", 
        "attributes": [
            "uid", 
            "eduPersonEntitlement", 
            "eduPersonPrincipalName", 
            "eduPersonAffiliation", 
            "cn", 
            "givenName", 
            "sn", 
            "mail", 
            "schacHomeOrganization"
        ], 
        "authproc": {
            "1": {
                "class": "saml:TransientNameID"
            }, 
            "2": {
                "attribute": "eduPersonPrincipalName", 
                "class": "saml:PersistentNameID"
            }
        }, 
        "entityid": "{BASE_URL}/sspsp/module.php/saml/sp/metadata.php/default-sp", 
        "name": "Service Provider for '{DOMAIN_NAME}'"
    }
]
